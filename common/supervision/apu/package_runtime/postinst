[ -f $ROOTACT/etc/supervision/supervision.cfg.new ] && \
rm -f $ROOTACT/etc/supervision/supervision.cfg.new 
mv -f $ROOTACT/etc/supervision/supervision.cfg $ROOTACT/etc/supervision/supervision.cfg.new
[ -f $ROOTACT/etc/supervision/supervision.cfg.preinst ] && \
mv -f $ROOTACT/etc/supervision/supervision.cfg.preinst $ROOTACT/etc/supervision/supervision.cfg
[ -f $ROOTACT/etc/supervision/supervision.cfg ] || \
cp $ROOTACT/etc/supervision/supervision.cfg.new $ROOTACT/etc/supervision/supervision.cfg

